Host Github personal
  HostName github.com
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/github-personal
 
Host Github corva
  HostName github.com
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/github-corva

Host Gitlab personal
  HostName gitlab.com
  UseKeychain yes
  AddKeysToAgent yes
  Preferredauthentications publickey
  IdentityFile ~/.ssh/gitlab-personal


