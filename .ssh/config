Host Github personal
  HostName github.com
  UseKeychain yes
  AddKeysToAgent yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/github-personal
 
Host Gitlab personal
  HostName gitlab.com
  UseKeychain yes
  AddKeysToAgent yes
  Preferredauthentications publickey
  IdentitiesOnly yes
  IdentityFile ~/.ssh/gitlab-personal

Host Gitlab Anadea
  HostName https://newgit.anadea.co/
  UseKeychain yes
  AddKeysToAgent yes
  Preferredauthentications publickey
  IdentitiesOnly yes
  IdentityFile ~/.ssh/gitlab-anadea
