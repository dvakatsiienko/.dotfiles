{
    "name": "dotfiles",
    "version": "0.0.1",
    "author": "Dima Vakatsiienko <imagnum.satellite@gmail.com>",
    "scripts": {
        "install-macos": "zx ./script/install-macos.mjs",
        "install-dotfiles": "zx ./script/install-dotfiles.mjs",
        "list-symlinks": "zx ./script/list-symlinks.mjs",
        "untrack-dotfile": "zx ./script/untrack-dotfile.mjs",
        "lib:add": "python3 ./membank/libsource/core/libsource_add.py",
        "lib:list": "python3 ./membank/libsource/core/libsource_list.py",
        "lib:update": "python3 ./membank/libsource/core/libsource_update.py",
        "lib:delete": "python3 ./membank/libsource/core/libsource_delete.py",
        "lib:restore": "python3 ./membank/libsource/core/libsource_restore.py",
        "lib:search": "python3 ./membank/cli/search.py",
        "lib:server": "python3 ./membank/cli/server.py",
        "rag:start": "python3 ./membank/cli/server.py start",
        "rag:stop": "python3 ./membank/cli/server.py stop",
        "rag:status": "python3 ./membank/cli/server.py status",
        "rag:restart": "python3 ./membank/cli/server.py restart",
        "sline:build": "cd ./.clauderc/sline && go build -o bin main.go",
        "check": "biome check --write"
    },
    "dependencies": {
        "zx": "8.8.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.0",
        "biome-config-polished": "0.0.2",
        "prettier": "3.6.2",
        "prettier-config-polished": "0.0.6"
    },
    "engines": {
        "pnpm": ">=10.14.0",
        "node": ">=22.17.0"
    }
}
