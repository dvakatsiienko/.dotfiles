{
    "name": "dotfiles",
    "version": "0.0.1",
    "author": "Dima Vakatsiienko <imagnum.satellite@gmail.com>",
    "scripts": {
        "install-macos": "zx ./script/install-macos.mjs",
        "install-dotfiles": "zx ./script/install-dotfiles.mjs",
        "list-symlinks": "zx ./script/list-symlinks.mjs",
        "untrack-dotfile": "zx ./script/untrack-dotfile.mjs",
        "mem:list": "python3 ./membank/cli/libsource_list.py",
        "mem:add": "python3 ./membank/cli/libsource_add.py",
        "mem:read": "python3 ./membank/cli/libsource_read.py",
        "mem:update": "python3 ./membank/cli/libsource_update.py",
        "mem:delete": "python3 ./membank/cli/libsource_delete.py",
        "mem:search": "python3 ./membank/cli/membank_search.py",
        "mem:server": "python3 ./membank/cli/membank_server.py",
        "mem:server:start": "python3 ./membank/cli/membank_server.py start",
        "mem:server:stop": "python3 ./membank/cli/membank_server.py stop",
        "mem:server:status": "python3 ./membank/cli/membank_server.py status",
        "mem:server:restart": "python3 ./membank/cli/membank_server.py restart",
        "mem:db:init": "python3 ./membank/cli/membank_db_init.py",
        "mem:db:explore": "python3 ./membank/cli/membank_db_explore.py",
        "sline:build": "cd ./.clauderc/sline && go build -o bin main.go",
        "check": "biome check --write"
    },
    "dependencies": {
        "@orama/orama": "3.1.14",
        "zx": "8.8.1"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.4",
        "biome-config-polished": "0.0.2",
        "prettier": "3.6.2",
        "prettier-config-polished": "0.0.6"
    },
    "engines": {
        "pnpm": ">=10.14.0",
        "node": ">=22.17.0"
    }
}
